#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int nums[n];
    printf("Enter %d elements: ", n);
    for (int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    int leftProd[n], rightProd[n], answer[n];

  
    leftProd[0] = 1;
    for (int i = 1; i < n; i++)
        leftProd[i] = leftProd[i - 1] * nums[i - 1];

  
    rightProd[n - 1] = 1;
    for (int i = n - 2; i >= 0; i--)
        rightProd[i] = rightProd[i + 1] * nums[i + 1];

  
    for (int i = 0; i < n; i++)
        answer[i] = leftProd[i] * rightProd[i];

  
    printf("Product array: ");
    for (int i = 0; i < n; i++) {
        printf("%d", answer[i]);
        if (i != n - 1)
            printf(", ");
    }
    printf("\n");

    return 0;
}
